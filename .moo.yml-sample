commit:
  style: CategorisedStyle # options: ImperativeMoodStyle, CategorisedStyle
  categories:
    One: One
    Bug: Bug
  words:
    - Change
    - Add
    - Create
    - Update
    - Build
    - Remove
    - Set
    - Temporarily
    - Rename
    - Implement
    - Reduce
    - Combine
    - Move
    - Configure
    - Limit
    - Exclude
    - Place
    - Use
    - Reset
    - Fix
    - Hide
    - Clean
    - Init
    - Make
    - Integrate
    - Enable
    - Disable
    - Reinstate
